<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="org.scoula.codefapi.bml.mapper.BuildingManagementLedgerMapper">

    <insert id="insertBuildingData" parameterType="org.scoula.codefapi.bml.dto.BuildingManagementLedgerDto">
        INSERT INTO BML_tbl (
            bml_violation_structure,
            bml_owner_addr,
            bml_use_type
        ) VALUES (
            #{resViolationStatus},
            #{resUserAddr},
            #{resContents}
        )
    </insert>

</mapper>
